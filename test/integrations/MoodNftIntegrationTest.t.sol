pragma solidity 0.8.20;

import {Test, console} from "forge-std/Test.sol";
import {MoodNft} from "src/MoodNft.sol";
import {DeployMoodNft} from "script/DeployMoodNft.s.sol";

contract MoodNftIntegrationTest is Test {
    MoodNft moodNft;
    DeployMoodNft deployer;

    address user1 = makeAddr("user1");
    address user2 = makeAddr("user2");

    string public constant SAD_SVG_IMAGE_URI =
        "data:image/svg+xml;base64,";

    string public constant HAPPY_SVG_IMAGE_URI =
        "data:image/svg+xml;base64,";

    string public constant SAD_SVG_URI =
        "data:application/json;base64,";

    string public constant HAPPY_SVG_URI =
        "data:application/json;base64,";

    function setUp() public {
        deployer = new DeployMoodNft();
        moodNft = deployer.run();
    }

    function testIntegrationViewTokenUri() public {
        vm.prank(user1);
        moodNft.mintNft();
        assertEq(moodNft.tokenURI(0), HAPPY_SVG_URI);
    }

    function testFlipMood() public {
        vm.prank(user1);
        moodNft.mintNft();
        console.log(moodNft.tokenURI(0));
        vm.prank(user1);
        moodNft.flipMood(0);
        assertEq(moodNft.tokenURI(0), SAD_SVG_URI);
    }

    function testOwnerCanFlipMoodMultipleTimes() public {
        vm.prank(user1);
        moodNft.mintNft();

        vm.prank(user1);
        moodNft.flipMood(0);
        assertEq(moodNft.tokenURI(0), SAD_SVG_URI);

        vm.prank(user1);
        moodNft.flipMood(0);
        assertEq(moodNft.tokenURI(0), HAPPY_SVG_URI);

        vm.prank(user1);
        moodNft.flipMood(0);
        assertEq(moodNft.tokenURI(0), SAD_SVG_URI);
    }

    function testUnauthorizedFlipMoodReverts() public {
        vm.prank(user1);
        moodNft.mintNft();

        // A different address attempts to flip mood
        address attacker = makeAddr("attacker");
        vm.prank(attacker);
        vm.expectRevert(MoodNft.MoodNft__CantFlipMoodIfNotOwner.selector);
        moodNft.flipMood(0);
    }

    function testMultipleNftsDifferentMoods() public {
        // Mint NFT for user 1
        vm.prank(user1);
        moodNft.mintNft();

        // Mint NFT for user 2
        vm.prank(user2);
        moodNft.mintNft();

        // Flip mood for NFT 0
        vm.prank(user1);
        moodNft.flipMood(0);
        assertEq(moodNft.tokenURI(0), SAD_SVG_URI);

        // Ensure NFT 1 remains HAPPY
        assertEq(moodNft.tokenURI(1), HAPPY_SVG_URI);
    }

    function testTokenCounterIncrements() public {
        vm.prank(user1);
        moodNft.mintNft();
        assertEq(moodNft.getTokenCounter(), 1);

        vm.prank(user1);
        moodNft.mintNft();
        assertEq(moodNft.getTokenCounter(), 2);
    }

    function testRevertOnInvalidTokenId() public {
        // Try to flip mood for a non-existent token
        vm.expectRevert(MoodNft.MoodNft__CantFlipMoodIfNotOwner.selector);
        moodNft.flipMood(999);
    }

    function testConstructorInitialization() public view {
        // Act
        // Assert
        assertEq(moodNft.name(), "Mood"); // Check the name of the NFT
        assertEq(moodNft.symbol(), "MOOD"); // Check the symbol of the NFT
        assertEq(moodNft.getTokenCounter(), 0); // Check the initial token counter
        assertEq(moodNft.getSadSvgImageUri(), SAD_SVG_IMAGE_URI); // Check for Happy mood URI
        assertEq(moodNft.getHappySvgImageUri(), HAPPY_SVG_IMAGE_URI); // Check for Sad mood URI
    }
}
